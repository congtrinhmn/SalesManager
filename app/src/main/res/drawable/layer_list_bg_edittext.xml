<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape android:shape="rectangle">
            <solid android:color="#00ffffff" />
            <stroke
                android:width="1dp"
                android:color="#ff9e9e9e" />
            <padding
                android:bottom="8dp"
                android:left="5dp"
                android:right="5dp"
                android:top="8dp" />
        </shape>
    </item>
</layer-list>
