<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="600dp"
    android:layout_height="48dp"
    android:clickable="true">

    <TextView
        android:id="@+id/tv_ma_sp"
        style="@style/textTableItem"
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/layer_list_border_item"
        android:text="@string/ma_sp" />

    <TextView
        android:id="@+id/tv_ten_sp"
        style="@style/textTableItem"
        android:layout_width="180dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/layer_list_border_item"
        android:text="@string/ten_sp" />

    <TextView
        android:id="@+id/tv_sl_nhap"
        style="@style/textTableItem"
        android:layout_width="70dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/layer_list_border_item"
        android:text="@string/sl_nhap" />

    <TextView
        android:id="@+id/tv_sl_ton"
        style="@style/textTableItem"
        android:layout_width="70dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/layer_list_border_item"
        android:text="@string/sl_ton" />

    <TextView
        android:id="@+id/tv_gia_dx"
        style="@style/textTableItem"
        android:layout_width="110dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/layer_list_border_item"
        android:text="@string/gia_de_xuat" />

    <TextView
        android:id="@+id/tv_gia_nhap"
        style="@style/textTableItem"
        android:layout_width="110dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/layer_list_border_item"
        android:text="@string/gia_nhap" />


</LinearLayout>